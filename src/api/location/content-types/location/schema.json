{
  "kind": "collectionType",
  "collectionName": "locations",
  "info": {
    "singularName": "location",
    "pluralName": "locations",
    "displayName": "Location"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "city": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "address": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "phone": {
      "type": "string",
      "regex": "/^\\+?(\\d[\\d-. ]+)?(\\([\\d-. ]+\\))?[\\d-. ]+\\d$/",
      "required": true,
      "unique": true
    }
  }
}
